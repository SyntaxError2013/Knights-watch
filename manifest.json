{
  "manifest_version": 2,
  "name": "tabZhub",
  "description": "Save and share tabs",
  "version": "1.0",

 "permissions": [
    "tabs",
    "notifications",
    "*://tabzhub.appspot.com/*"
  ],

  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "popup.html",
    "default_name": "tabZhub",
    "icons": ["icon.png"]
  }

}
