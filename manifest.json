{
  "manifest_version": 2,

  "name": "tabZhub",
  "description": "Save and share tabs",
  "version": "1.0",

  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "popup.html"
  },
  
  "content_scripts": [
    {
      "matches": ["//ajax.googleapis.com/*"],
      "js": [
              "jquery.min.js", 
              "jquery-ui.min.js",
              "popup.js"
            ]
    }
  ],

  "permissions": [
    "tabs",
	  "http://tabzhub.appspot.com/*",
    "http://www.google.com/*"
  ]
}
