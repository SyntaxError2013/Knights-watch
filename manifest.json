{
  "manifest_version": 2,

  "name": "tabZhub",
  "description": "Save and share tabs",
  "version": "1.0",

 "permissions": [
    "tabs",
	  "http://tabzhub.appspot.com/*",
    "*://*/*"
  ],

  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "popup.html"
  },
  
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "css": ["jquery-ui.css"],
      "js": ["jquery-1.9.1.js", "jquery-ui.js", "popup.js"]
    }
  ]
}
