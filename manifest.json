{
  "manifest_version": 2,

  "name": "tabZhub",
  "description": "Save and share tabs",
  "version": "1.0",

 "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],

  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "popup.html"
  },

  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["jquery-1.9.1.js", "jquery-ui.js", "popup.js"],
      "run_at": "document_start"
    }
  ],

}
